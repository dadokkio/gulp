        "PreviousRun4": {
          "is_timestamp": true,
          "event_code": "previous_run_4"
        },
        "PreviousRun5": {
          "is_timestamp": true,
          "event_code": "previous_run_5"
        },
        "PreviousRun6": {
          "is_timestamp": true,
          "event_code": "previous_run_6"
        },
        "Volume0Created": {
          "is_timestamp": true,
          "event_code": "volume0_created"
        },
        "Volume1Created": {
          "is_timestamp": true,
          "event_code": "volume1_created"
        },
        "Directories": {
          "map_to": "file.directory"
        },
        "FilesLoaded": {}
      },
      "options": {
        "default_event_code": "pecmd",
        "agent_type": "PECmd",
        "mapping_id": "pecmd"
      }
