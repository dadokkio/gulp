{
    "metadata": {
      "plugin": [
        "lin_syslog.py"
      ]
    },
    "mappings": {
      "syslog": {
        "event_code": "process.name",
        "agent_type": "lin_syslog",
        "fields": {
          "hostname": {
            "ecs": "host.hostname"
          },
          "pid": {
            "ecs": "process.pid"
          },
          "process": {
            "ecs": "process.name"
          }
        }
      }
    }
  }