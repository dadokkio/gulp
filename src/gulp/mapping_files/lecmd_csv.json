{
  "metadata": {
    "plugin": [
      "csv.py"
    ]
  },
  "mappings": {
    "lecmd": {
      "event_code": "source_created",
      "agent_type": "lecmd",
      "fields": {
        "SourceFile": {
          "ecs": "file.path"
        },
        "SourceCreated": {
          "ecs": "@timestamp"
        },
        "SourceModified": {
          "extra_doc_with_event_code": "source_modified"
        },
        "SourceAccessed": {
          "extra_doc_with_event_code": "source_accessed"
        },
        "TargetCreated": {
          "extra_doc_with_event_code": "target_created"
        },
        "TargetModified": {
          "extra_doc_with_event_code": "target_modified"
        },
        "TargetAccessed": {
          "extra_doc_with_event_code": "target_accessed"
        },
        "FileSize": {
          "ecs": "file.size"
        },
        "MachineID": {
          "ecs": "host.id"
        },
        "MachineMACAddress": {
          "ecs": "host.mac"
        },
        "TrackerCreatedOn": {
          "extra_doc_with_event_code": "target_created",
          "event_code": "tracker_created"
        }
      }
    }
  }
}