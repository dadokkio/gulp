{
  "metadata": {
    "plugin": ["gi_csv.py"]
  },
  "mappings": [
    {
      "fields": {
        "SourceFile": {
          "map_to": "file.path"
        },
        "SourceCreated": {
          "is_timestamp": true,
          "event_code": "created"
        },
        "SourceModified": {
          "is_timestamp": true,
          "event_code": "modified"
        },
        "SourceAccessed": {
          "is_timestamp": true,
          "event_code": "accessed"
        }
      },
      "options": {
        "default_event_code": "recentfilecacheparser",
        "agent_type": "RecentFileCacheParser",
        "mapping_id": "recentfilecacheparser"
      }
    }
  ]
}
